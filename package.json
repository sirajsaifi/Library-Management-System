{
  "name": "librarymanagment",
  "version": "1.0.0",
  "description": "Creating a Library Management System using node.js, express js, mongoDB, pug and css",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb debug",
    "watch:js": "parcel watch ./public/js/index.js --dist-dir ./public/js/bundled",
    "build:js": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled"
  },
  "author": "Siraj Saifi",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.35.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.14.1",
    "sharp": "^0.33.2",
    "slugify": "^1.6.6",
    "validator": "^13.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "parcel": "^2.11.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">=v21.6.1"
  }
}
